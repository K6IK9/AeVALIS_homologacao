{
    "version": 2,
    "builds": [
        {
            "src": "setup/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "setup/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "setup.settings"
    },
    "functions": {
        "setup/wsgi.py": {
            "maxDuration": 30
        }
    }
}