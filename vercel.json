{
    "version": 2,

    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "setup/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "setup.settings"
    }
}